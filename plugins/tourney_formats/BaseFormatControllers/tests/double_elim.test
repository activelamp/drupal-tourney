<?php

class DoubleEliminationNextMatchTestCase extends DrupalWebTestCase {
  /**
   * Test info.
   */
  public static function getInfo() {
    return array(
      'name' => t('Test Double Elimination Challonge next match positions'),
      'description' => t('Tests double elimination tournaments with a varying number or contestants.'),
      'group' => t('Tourney'),
    );
  }
  
  /**
   * Set up test.
   */
  public function setUp() {
    module_load_include('php', 'tourney', 'plugins/tourney_formats/BaseFormatControllers/DoubleElimination');
    parent::setUp(array(
      'tourney',
    ));
  }
  
  /**
   * Loops through all the matches defined in the test and checks values against
   * the plugin.
   * 
   * @param $match
   *   A formatted match array that has a winner and slot.
   * @param $tournament
   *   The built tournament object.
   */
  protected function checkNextMatch($match, $tournament) {
    $this->pass('Testing Double Elimination with ' . $tournament->numContestants . ' players.');
    $matches = $tournament->data['matches'];
    
    foreach ($match as $mid => $info) {
      $plugin_winner = $matches[$mid]['nextMatch']['winner']['id'];
      $this->assertEqual($info['winner_id'], $plugin_winner, 'Match ' . $mid . ' winner is going to match ' . $info['winner_id'] . ' (Match ' . $plugin_winner .')', 'PHP');
      //$this->assertEqual($info['winner_slot'], $matches[$mid]['nextMatch']['winner']['slot'], 'Match ' . $mid . ' winner is going to slot' . $info['winner_slot'], 'PHP');
      if (array_key_exists('loser_id', $info)) {
        $plugin_loser = $matches[$mid]['nextMatch']['loser']['id'];
        $this->assertEqual($info['loser_id'], $plugin_loser, 'Match ' . $mid . ' loser is going to match ' . $info['loser_id'] . ' (Match ' . $plugin_loser .')', 'PHP');
        //$this->assertEqual($info['loser_slot'], $matches[$mid]['nextMatch']['loser']['slot'], 'Match ' . $mid . ' loser is going to slot' . $info['loser_slot'], 'PHP');
      }
    }
  }
  
